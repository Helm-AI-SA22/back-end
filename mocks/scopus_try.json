                "dc:title": "Symptoms as Solutions: Hypnosis and Biofeedback for Autonomic Regulation in Autism Spectrum Disorders",
                "dc:creator": "Sugarman, L.I.",
                "prism:publicationName": "American Journal of Clinical Hypnosis",
                "prism:issn": "00029157",
